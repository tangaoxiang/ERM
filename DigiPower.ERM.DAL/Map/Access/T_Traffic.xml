  <statements>
    <select id="QueryTraffic_ByProjID" parameterClass="String" resultMap="FullResultMap">
      SELECT * FROM [T_Traffic] WHERE
      ([T_Traffic].[ProjectID] = #value#)
    </select>


    <insert id="InsertT_Traffic" parameterClass="T_Traffic">
      Insert Into T_Traffic (
      ID,
      Road_Level,
      LongCount,
      Width,
      HZ,
      JCXS,
      MCXS,
      KJ,
      KS,
      ZPWS,
      PWDJ,
      ProjectID,
      Remark,
      GCYS,
      GCJS,
      STARTX,
      STARTY,
      ENDX,
      ENDY,
      HJQK,
      GG,
      SGYL,
      CD,
      ZL,
      CZ
      )Values(
      #ID#,
      #Road_Level#,
      #LongCount#,
      #Width#,
      #HZ#,
      #JCXS#,
      #MCXS#,
      #KJ#,
      #KS#,
      #ZPWS#,
      #PWDJ#,
      #ProjectID#,
      #Remark#,
      #GCYS#,
      #GCJS#,
      #STARTX#,
      #STARTY#,
      #ENDX#,
      #ENDY#,
      #HJQK#,
      #GG#,
      #SGYL#,
      #CD#,
      #ZL#,
      #CZ#
      )

    </insert>

    <update id="UpdateT_Traffic" parameterClass="T_Traffic">
      Update T_Traffic Set
      Road_Level=#Road_Level#,
      LongCount=#LongCount#,
      Width=#Width#,
      HZ=#HZ#,
      JCXS=#JCXS#,
      MCXS=#MCXS#,
      KJ=#KJ#,
      KS=#KS#,
      ZPWS=#ZPWS#,
      PWDJ=#PWDJ#,
      Remark=#Remark#,
      GCYS=#GCYS#,
      GCJS=#GCJS#,
      STARTX=#STARTX#,
      STARTY=#STARTY#,
      ENDX=#ENDX#,
      ENDY=#ENDY#,
      HJQK= #HJQK#,
      GG=#GG#,
      SGYL=#SGYL#,
      CD= #CD#,
      ZL=#ZL#,
      CZ=#CZ#
      <dynamic prepend="WHERE">
        <isParameterPresent>
          ID=#ID#
        </isParameterPresent>
      </dynamic>
    </update>

    <delete id="DeleteT_Traffic" parameterClass="String">
      Delete From T_Traffic
      <dynamic prepend="WHERE">
        <isParameterPresent>
          ID=#value#
        </isParameterPresent>
      </dynamic>
    </delete>
    <delete id="DeleteTrafficByProjectNO" parameterClass="String">
      Delete From T_Traffic
      <dynamic prepend="WHERE">
        <isParameterPresent>
          ProjectID=#value#
        </isParameterPresent>
      </dynamic>
    </delete>
  </statements>
